{
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "model": "opus",
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "printf '\\a'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "ccusage statusline"
  },
  "enabledPlugins": {
    "llm-rules@Meta": true,
    "meta@Meta": true
  }
}
